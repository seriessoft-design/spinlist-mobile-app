# Resolve react_native_pods.rb with node to allow for hoistingrequire Pod::Executable.execute_command('node', ['-p',  'require.resolve(    "react-native/scripts/react_native_pods.rb",    {paths: [process.argv[1]]},  )', __dir__]).stripplatform :ios, min_ios_version_supportedprepare_react_native_project!# Use dynamic frameworks for Swift pods compatibilityuse_frameworks! :linkage => :statictarget 'SpinlistApp' do  config = use_native_modules!  use_react_native!(    :path => config[:reactNativePath],    # An absolute path to your application root.    :app_path => "#{Pod::Config.instance.installation_root}/.."  )  post_install do |installer|    react_native_post_install(      installer,      config[:reactNativePath],      :mac_catalyst_enabled => false,      # :ccache_enabled => true    )  endend
